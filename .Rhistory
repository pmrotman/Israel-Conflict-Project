library(HSAUR2)
data(heptathlon)
heptathlon$hurdles <- max(heptathlon$hurdles) - heptathlon$hurdles
heptathlon$run200m <- max(heptathlon$run200m) - heptathlon$run200m
heptathlon$run800m <- max(heptathlon$run800m) - heptathlon$run800m
score <- which(colnames(heptathlon) == "score")
heptathlon <- heptathlon[-grep("PNG", rownames(heptathlon)),]
#round(cor(heptathlon[,-score]), 2)
hep_pca <- prcomp(heptathlon[,-score], scale = TRUE)
hep_matrix <- as.matrix(heptathlon[,-score])
#drop(scale(hep_matrix, center = hep_pca$center, scale = hep_pca$scale)) %*% hep_pca$rotation[,1]
#predict(hep_pca)[,1]
paste("Correlation between score and PCA", cor(heptathlon$score, hep_pca$x[,1]))
biplot(hep_pca, col = c("black", "red"),
xlim = c(-0.6, 0.6), ylim = c(-0.5, 0.5), cex = 0.5)
summary(hep_pca)
apply(heptathlon, 2, sd)
